{
  "name": "@deltecho/integrations",
  "version": "1.0.0",
  "description": "Platform integrations for Deep Tree Echo - Discord, Telegram, and WebGPU inference",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./discord": {
      "import": "./dist/discord/index.js",
      "types": "./dist/discord/index.d.ts"
    },
    "./telegram": {
      "import": "./dist/telegram/index.js",
      "types": "./dist/telegram/index.d.ts"
    },
    "./webgpu": {
      "import": "./dist/webgpu/index.js",
      "types": "./dist/webgpu/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "check:types": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "discord",
    "telegram",
    "webgpu",
    "ai",
    "deep-tree-echo",
    "chatbot",
    "integration",
    "platform"
  ],
  "author": "",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@deltecho/cognitive": "workspace:*",
    "deep-tree-echo-core": "workspace:*",
    "discord.js": "^14.18.0",
    "@anthropic-ai/sdk": "^0.52.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^25.0.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.30.0"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/sdk": {
      "optional": true
    }
  }
}
