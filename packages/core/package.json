{
  "name": "deep-tree-echo-core",
  "version": "1.0.0",
  "description": "Core cognitive and memory modules for Deep Tree Echo AI system",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cognitive": {
      "import": "./dist/cognitive/index.js",
      "types": "./dist/cognitive/index.d.ts"
    },
    "./memory": {
      "import": "./dist/memory/index.js",
      "types": "./dist/memory/index.d.ts"
    },
    "./personality": {
      "import": "./dist/personality/index.js",
      "types": "./dist/personality/index.d.ts"
    },
    "./security": {
      "import": "./dist/security/index.js",
      "types": "./dist/security/index.d.ts"
    },
    "./embodiment": {
      "import": "./dist/embodiment/index.js",
      "types": "./dist/embodiment/index.d.ts"
    },
    "./adapters": {
      "import": "./dist/adapters/index.js",
      "types": "./dist/adapters/index.d.ts"
    },
    "./integration": {
      "import": "./dist/integration/index.js",
      "types": "./dist/integration/index.d.ts"
    },
    "./config": {
      "import": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    },
    "./multimodal": {
      "import": "./dist/multimodal/index.js",
      "types": "./dist/multimodal/index.d.ts"
    },
    "./consciousness": {
      "import": "./dist/consciousness/index.js",
      "types": "./dist/consciousness/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "check:types": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage"
  },
  "keywords": [
    "ai",
    "cognitive-architecture",
    "memory-systems",
    "personality",
    "deltachat"
  ],
  "author": "",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^25.0.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
