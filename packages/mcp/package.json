{
  "name": "deep-tree-echo-mcp",
  "version": "1.0.0",
  "description": "Multi-layer nested MCP server for AAR architecture",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "deep-tree-echo-mcp": "./dist/bin/mcp-server.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "start": "node dist/bin/mcp-server.js",
    "start:stdio": "node dist/bin/mcp-server.js --stdio",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "deep-tree-echo-orchestrator": "workspace:*",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  },
  "files": [
    "dist",
    "docs",
    "README.md",
    "INTEGRATION.md"
  ],
  "keywords": [
    "mcp",
    "model-context-protocol",
    "aar",
    "agent",
    "arena",
    "relation",
    "deep-tree-echo",
    "lifecycle",
    "inverted-mirror"
  ]
}
